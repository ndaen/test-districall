<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Test Districall</title>
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<nav class="nav">
    <ul class="nav-list">
        {% if app.user %}
            <li class="nav-item-list"><a href="{{ path('app_logout') }}">Log Out</a></li>
        {% else %}
            <li class="nav-item-list"><a href="{{ path('app_login') }}">Log In</a></li>
            <li class="nav-item-list"><a href="{{ path('app_register') }}">Sign Up</a></li>
        {% endif %}
        <li class="nav-item-list"><a href="{{ path('app_home') }}">Home</a></li>
    </ul>
</nav>
{% block body %}{% endblock %}
</body>
</html>
